{
  "name": "chrysalis-meditation-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A comprehensive meditation and mindfulness app with social features, QR challenges, and personalized guidance",
  "keywords": [
    "meditation",
    "mindfulness",
    "wellness",
    "PWA",
    "social",
    "QR codes"
  ],
  "author": "Chrysalis Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "cd server && npm install && npm run build",
    "start": "cd server && npm start",
    "build:frontend": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "deploy": "./scripts/deploy.sh deploy",
    "deploy:preview": "netlify deploy --dir=dist",
    "deploy:prod": "netlify deploy --prod --dir=dist",
    "serve": "npm run build && npm run preview",
    "clean": "rm -rf dist node_modules/.cache .vite",
    "demo": "node scripts/demo-guide.cjs",
    "demo:features": "echo 'ðŸ§˜ Chrysalis Features: Social Feed (/social), Leaderboard (/leaderboard), QR Connect (/qr), Profile (/profile)'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.11.1",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.35.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^0.0.48",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "@types/qrcode.react": "^1.0.5",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "framer-motion": "^12.23.0",
    "helmet": "^8.1.0",
    "idb": "^8.0.3",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.525.0",
    "multer": "^2.0.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.1",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-webcam": "^7.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
